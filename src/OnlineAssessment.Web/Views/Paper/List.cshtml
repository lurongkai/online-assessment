@model IEnumerable<ExaminationPaper>
<p>
    @Html.ActionLink("新建", "Create", null, new { @class = "btn btn-primary" })
</p>
<p>
    <table class="table table-hover table-bordered">
        <tr>
            <th>用户名</th>
            <th>姓名</th>
            <th>角色</th>
            <th>操作</th>
        </tr>
        @foreach (var item in Model) {
            <tr>
                <td>
                    @item.Title
                </td>
                <td>
                    @item.TotalScore
                </td>
                <td>
                    @item.Degree
                </td>
                <td>
                    @Html.ActionLink("添加考试", "NewExamination", new { paperId = item.ExaminationPaperId }, new { @class = "btn btn-danger btn-xs" })
                    @Html.ActionLink("删除", "Delete", new { paperId = item.ExaminationPaperId }, new { @class = "btn btn-danger btn-xs" })
                </td>
            </tr>
        }
    </table>
</p>