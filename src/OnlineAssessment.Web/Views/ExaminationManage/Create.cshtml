@model OnlineAssessment.Service.Message.ExaminationConfig

@using (Html.BeginForm("Create", "ExaminationManage", new { paperId = ViewBag.paperId }, FormMethod.Post, new { @class = "form-horizontal" }))
{
    <div class="form-group">
        @Html.LabelFor(m => m.Title, "考试标题", new { @class = "col-sm-2 control-label" })
        <div class="col-sm-10">
            @Html.TextBoxFor(m => m.Title, new { @class = "form-control", placeholder = "考试标题" })
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.BeginDate, "开始日期", new { @class = "col-sm-2 control-label" })
        <div class="col-sm-10">
            @Html.TextBoxFor(m => m.BeginDate, new { @class = "form-control", placeholder = "开始日期" })
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.EndDate, "结束日期", new { @class = "col-sm-2 control-label" })
        <div class="col-sm-10">
            @Html.TextBoxFor(m => m.EndDate, new { @class = "form-control", placeholder = "结束日期" })
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Duration, "考试时长", new { @class = "col-sm-2 control-label" })
        <div class="col-sm-10">
            @Html.TextBoxFor(m => m.Duration, new { @class = "form-control", placeholder = "考试时长" })
        </div>
    </div>
    <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
            <button type="submit" class="btn btn-primary">添加</button>
        </div>
    </div>
}


